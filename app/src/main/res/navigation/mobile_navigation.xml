<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_home">

    <fragment
        android:id="@+id/navigation_home"
        android:name="smyo.app.ui.home.HomeFragment"
        android:label="@string/title_home"
        tools:layout="@layout/fragment_home" />

    <fragment
        android:id="@+id/navigation_trading"
        android:name="smyo.app.ui.trading.TradingFragment"
        android:label="@string/title_trading"
        tools:layout="@layout/fragment_trading" />

    <fragment
        android:id="@+id/navigation_addoutfit"
        android:name="smyo.app.ui.addoutfit.AddOutfitFragment"
        android:label="@string/title_addoutfit"
        tools:layout="@layout/fragment_addoutfit" />
</navigation>
